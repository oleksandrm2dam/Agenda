package util;

import java.io.FileNotFoundException;
import java.io.FileOutputStream;

import entities.Phonebook;

public class PhonebookUtil {
	
	private static String phonebookPath = "/files/phonebook.pb";
	
	public static Phonebook readPhonebook() {
		return new Phonebook();
	}
	
	public static void writePhonebook(Phonebook phonebook) {
		try {
			FileOutputStream fos = new FileOutputStream(phonebookPath);
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
	
}
